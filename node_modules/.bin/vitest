#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/raghu/dev/sandbox/ghu/node_modules/.pnpm/vitest@4.0.2_@types+node@24.9.1_tsx@4.20.6_yaml@2.8.1/node_modules/vitest/node_modules:/Users/raghu/dev/sandbox/ghu/node_modules/.pnpm/vitest@4.0.2_@types+node@24.9.1_tsx@4.20.6_yaml@2.8.1/node_modules:/Users/raghu/dev/sandbox/ghu/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/raghu/dev/sandbox/ghu/node_modules/.pnpm/vitest@4.0.2_@types+node@24.9.1_tsx@4.20.6_yaml@2.8.1/node_modules/vitest/node_modules:/Users/raghu/dev/sandbox/ghu/node_modules/.pnpm/vitest@4.0.2_@types+node@24.9.1_tsx@4.20.6_yaml@2.8.1/node_modules:/Users/raghu/dev/sandbox/ghu/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
